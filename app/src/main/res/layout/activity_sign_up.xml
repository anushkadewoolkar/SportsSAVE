<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_image"
    tools:context=".SignUpActivity">

    <EditText
        android:id="@+id/emailEditText"
        android:layout_width="355dp"
        android:layout_height="60dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Enter email"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="362dp" />

    <EditText
        android:id="@+id/passwordEditText"
        android:layout_width="313dp"
        android:layout_height="59dp"
        android:ems="10"
        android:hint="Make Password"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="551dp" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="346dp"
        android:layout_height="60dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="443dp" />

    <Button
        android:id="@+id/signUpButton"
        android:layout_width="297dp"
        android:layout_height="71dp"
        android:text="Create account"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="895dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="323dp"
        android:layout_height="149dp"
        app:srcCompat="@drawable/Button"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="596dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="198dp"
        android:layout_height="149dp"
        app:srcCompat="@drawable/SportsSEAlogo"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="38dp" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="362dp"
        android:layout_height="44dp"
        app:srcCompat="@drawable/Sportshaveneverbeensofun"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="266dp" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/SportsSEA"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="198dp" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="348dp"
        android:layout_height="78dp"
        app:srcCompat="@drawable/email"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="355dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="135dp"
        android:layout_height="26dp"
        android:text="Enter Email"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="385dp" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="348dp"
        android:layout_height="78dp"
        app:srcCompat="@drawable/email"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="443dp" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="348dp"
        android:layout_height="78dp"
        app:srcCompat="@drawable/email"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="532dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter Name"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="472dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Make Password"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="558dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/josefin_sans_thick"
        android:text="Sign Up"
        android:textColor="@color/cardview_light_background"
        android:textSize="40sp"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="640dp" />

</androidx.constraintlayout.widget.ConstraintLayout>