<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_image"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="362dp"
        android:layout_height="44dp"
        app:srcCompat="@drawable/Sportshaveneverbeensofun"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="256dp" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/SportsSEA"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="188dp" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="348dp"
        android:layout_height="78dp"
        app:srcCompat="@drawable/email"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="315dp" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="348dp"
        android:layout_height="78dp"
        app:srcCompat="@drawable/email"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="407dp" />

    <EditText
        android:id="@+id/email"
        android:layout_width="329dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="Email"
        android:inputType="textPersonName"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="329dp" />

    <EditText
        android:id="@+id/password"
        android:layout_width="338dp"
        android:layout_height="57dp"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="418dp" />

    <Button
        android:id="@+id/login"
        android:layout_width="352dp"
        android:layout_height="59dp"
        android:text="Login"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="530dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="171dp"
        android:layout_height="154dp"
        app:srcCompat="@drawable/SportsSEAlogo"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="34dp" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="352dp"
        android:layout_height="69dp"
        app:srcCompat="@drawable/button"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="530dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/josefin_sans_bold"
        android:text="Login"
        android:textColor="@color/cardview_light_background"
        android:textSize="50sp"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="535dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="302dp"
        android:layout_height="34dp"
        android:text="Don't have an account? Sign Up"
        android:textColor="#FA5858"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="620dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="246dp"
        tools:layout_editor_absoluteY="608dp" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="351dp"
        android:layout_height="133dp"
        app:srcCompat="@drawable/path2"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="285dp" />

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="116dp"
        android:layout_height="27dp"
        app:srcCompat="@drawable/Email"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="341dp" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="351dp"
        android:layout_height="133dp"
        app:srcCompat="@drawable/path2"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="379dp" />

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="114dp"
        android:layout_height="31dp"
        app:srcCompat="@drawable/Password"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="427dp" />

</androidx.constraintlayout.widget.ConstraintLayout>